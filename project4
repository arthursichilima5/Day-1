#include <stdio.h>

int main() {
    float celsius, fahrenheit;
    

    printf("250");
    scanf("%f,&celsius);

    
        
        
        fahrenheit = (celsius * 9 / 5.0) + 32;
        printf("In Fahrenheit: %.1f\n", fahrenheit);

        celsius = (fahrenheit - 32) * 5 / 9.0;
        printf("In Celsius: %.1f\n", celsius);
    
        printf("Invalid choice!\n");
    }

    return 0;
}
